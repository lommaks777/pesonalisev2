{
  "name": "webapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build --no-lint",
    "start": "next start",
    "lint": "eslint",
    "db:migrate": "tsx scripts/run-migrations.ts",
    "db:seed": "tsx scripts/import-lessons.ts",
    "db:update-lessons": "tsx scripts/update-lesson-numbers.ts",
    "db:update-lessons-api": "tsx scripts/update-lesson-numbers-api.ts",
    "db:update-lessons-safe": "tsx scripts/update-lesson-numbers-safe.ts",
    "db:fix-remaining": "tsx scripts/fix-remaining-lessons.ts",
    "db:update-user": "tsx scripts/update-user-personalizations.ts",
    "db:update-user-api": "tsx scripts/update-user-via-api.ts",
    "db:update-user-simple": "tsx scripts/update-user-simple.ts",
    "templates:regenerate": "tsx scripts/regenerate-lesson-templates.ts",
    "templates:generate": "tsx --env-file=.env.local scripts/generate-course-templates.ts",
    "course:process-transcripts": "tsx --env-file=.env.local scripts/process-course-transcripts.ts",
    "course:process-from-url": "tsx --env-file=.env.local scripts/process-course-from-url.ts",
    "course:check-videos": "tsx --env-file=.env.local scripts/check-kinescope-video-details.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed",
    "e2e:report": "playwright show-report"
